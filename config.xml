<?xml version="1.0" encoding="utf-8"?>
<widget id="com.kalelogistics.gmax" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>
		GMAX
	</name>
	<description>
		Android app for GMAX opertions.
	</description>
	<author email="sachin.semlety@kalelogistics.in" href="http://www.kalelogistics.in/">
		Sachin Semlety
	</author>
	<content src="index.html" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<preference name="ScrollEnabled" value="false" />
	<preference name="android-minSdkVersion" value="19" />
	<preference name="BackupWebStorage" value="none" />
	<preference name="SplashMaintainAspectRatio" value="true" />
	<preference name="FadeSplashScreenDuration" value="300" />
	<preference name="SplashShowOnlyFirstTime" value="false" />
	<preference name="SplashScreen" value="screen" />
	<preference name="SplashScreenDelay" value="3000" />
	<preference name="KeyboardDisplayRequiresUserAction" value="false" />
	<platform name="android">
		<allow-intent href="market:*" />
		<preference name="SplashMaintainAspectRatio" value="true" />
		<preference name="SplashShowOnlyFirstTime" value="true" />
		<icon density="ldpi" src="resources/icons/android/drawable-ldpi-icon.png" />
		<icon density="mdpi" src="resources/icons/android/drawable-mdpi-icon.png" />
		<icon density="hdpi" src="resources/icons/android/drawable-hdpi-icon.png" />
		<icon density="xhdpi" src="resources/icons/android/drawable-xhdpi-icon.png" />
		<icon density="xxhdpi" src="resources/icons/android/drawable-xxhdpi-icon.png" />
		<icon density="xxxhdpi" src="resources/icons/android/drawable-xxxhdpi-icon.png" />
		<splash density="port-ldpi" src="resources/screen/android/splash-port-ldpi.png" />
		<splash density="port-mdpi" src="resources/screen/android/splash-port-mdpi.png" />
		<splash density="port-hdpi" src="resources/screen/android/splash-port-hdpi.png" />
		<splash density="port-xhdpi" src="resources/screen/android/splash-port-xhdpi.png" />
		<splash density="port-xxhdpi" src="resources/screen/android/splash-port-xxhdpi.png" />
		<splash density="port-xxxhdpi" src="resources/screen/android/splash-port-xxxhdpi.png" />
		<splash density="land-ldpi" src="resources/screen/android/splash-land-ldpi.png" />
		<splash density="land-mdpi" src="resources/screen/android/splash-land-mdpi.png" />
		<splash density="land-hdpi" src="resources/screen/android/splash-land-hdpi.png" />
		<splash density="land-xhdpi" src="resources/screen/android/splash-land-xhdpi.png" />
		<splash density="land-xxhdpi" src="resources/screen/android/splash-land-xxhdpi.png" />
		<splash density="land-xxxhdpi" src="resources/screen/android/splash-land-xxxhdpi.png" />
	</platform>
	<platform name="ios">
		<preference name="CordovaWebViewEngine" value="CDVUIWebViewEngine" />
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
		<icon height="57" platform="ios" src="resources/icons/ios/icon.png" width="57" />
		<icon height="114" platform="ios" src="resources/icons/ios/icon@2x.png" width="114" />
		<icon height="40" platform="ios" src="resources/icons/ios/icon-40.png" width="40" />
		<icon height="80" platform="ios" src="resources/icons/ios/icon-40@2x.png" width="80" />
		<icon height="50" platform="ios" src="resources/icons/ios/icon-50.png" width="50" />
		<icon height="100" platform="ios" src="resources/icons/ios/icon-50@2x.png" width="100" />
		<icon height="60" platform="ios" src="resources/icons/ios/icon-60.png" width="60" />
		<icon height="120" platform="ios" src="resources/icons/ios/icon-60@2x.png" width="120" />
		<icon height="180" platform="ios" src="resources/icons/ios/icon-60@3x.png" width="180" />
		<icon height="72" platform="ios" src="resources/icons/ios/icon-72.png" width="72" />
		<icon height="144" platform="ios" src="resources/icons/ios/icon-72@2x.png" width="144" />
		<icon height="76" platform="ios" src="resources/icons/ios/icon-76.png" width="76" />
		<icon height="152" platform="ios" src="resources/icons/ios/icon-76@2x.png" width="152" />
		<icon height="29" platform="ios" src="resources/icons/ios/icon-small.png" width="29" />
		<icon height="58" platform="ios" src="resources/icons/ios/icon-small@2x.png" width="58" />
		<icon height="87" platform="ios" src="resources/icons/ios/icon-small@3x.png" width="87" />
		<splash src="resources/screen/ios/Default@2x~iphone~anyany.png" />
		<splash src="resources/screen/ios/Default@2x~iphone~comany.png" />
		<splash src="resources/screen/ios/Default@3x~iphone~anyany.png" />
		<splash src="resources/screen/ios/Default@3x~iphone~comany.png" />
		<splash src="resources/screen/ios/Default@2x~ipad~anyany.png" />
		<splash src="resources/screen/ios/Default@2x~ipad~comany.png" />
		<splash src="resources/screen/ios/Default@2x~iphone~comcom.png" />
		<splash src="resources/screen/ios/Default@3x~iphone~anycom.png" />
		<splash height="480" src="resources/screen/ios/Default~iphone.png" width="320" />
		<splash height="960" src="resources/screen/ios/Default@2x~iphone.png" width="640" />
		<splash height="1024" src="resources/screen/ios/Default-Portrait~ipad.png" width="768" />
		<splash height="2048" src="resources/screen/ios/Default-Portrait@2x~ipad.png" width="1536" />
		<splash height="1136" src="resources/screen/ios/Default-568h@2x~iphone.png" width="640" />
		<splash height="1334" src="resources/screen/ios/Default-667h.png" width="750" />
		<splash height="2208" src="resources/screen/ios/Default-736h.png" width="1242" />
		<splash height="768" src="resources/screen/ios/Default-Landscape~ipad.png" width="1024" />
		<splash height="1536" src="resources/screen/ios/Default-Landscape@2x~ipad.png" width="2048" />
	</platform>
	<platform name="windows">
		<splash src="resources/screen/windows/splashscreen.png" target="SplashScreen" />
		<splash src="resources/screen/windows/splashscreenphone.png" target="SplashScreenPhone" />
	</platform>
	<plugin name="cordova-plugin-whitelist" spec="1.3.3" />
	<plugin name="cordova-plugin-device" spec="2.0.2" />
	<plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
	<plugin name="cordova-plugin-ionic-webview" spec="1.1.19" />
	<plugin name="cordova-plugin-network-information" spec="^1.3.0" />
	<allow-navigation href="http://172.29.9.97:8100" />
	<allow-navigation href="http://10.22.3.64:8100" />
	<plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
	<plugin name="cordova-plugin-datepicker" spec="^0.9.3" />
	<plugin name="onesignal-cordova-plugin" spec="^2.4.1" />
	<engine name="android" spec="7.0.0" />
</widget>
